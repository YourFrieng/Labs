#include <iostream>
using namespace std;


int main()
{
	int number, sum1, sum2;
	sum1 = 0;
	sum2 = 0;
	cout << "Write number: ";
	cin >> number;
	for (int i = 0; i < 6; i++) {
		
		if (i < 3) {
			sum1 += number % 10;
		}
		else {
			sum2 += number % 10;
		}
		
		number /= 10;
	}

	if (sum1 == sum2) {

		cout << "Lucky";
	
	}
	else {
		
		cout << "Unlucky";

	}

	return 0;
}

